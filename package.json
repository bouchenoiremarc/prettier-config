{
  "name": "@bouchenoiremarc/prettier-config",
  "version": "1.1.0",
  "description": "My personal Prettier configuration.",
  "author": "Marc Bouchenoire",
  "license": "MIT",
  "repository": "https://github.com/bouchenoiremarc/prettier-config",
  "keywords": [
    "prettier",
    "config"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "prettier": "./index.js",
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "lint": "eslint '**/*.js' --fix",
    "release": "yarn login && np --no-tests"
  },
  "devDependencies": {
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "7.0.4",
    "lint-staged": "^11.2.6",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "prettier": ">=2.0.0"
  }
}
